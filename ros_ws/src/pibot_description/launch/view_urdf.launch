<launch>
	<param name="robot_description" textfile="$(find pibot_description)/urdf/$(env PIBOT_MODEL).urdf" />

	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />	
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
	
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find pibot_description)/config/view_urdf.rviz" required="true" />
</launch>
